{
    "env": "%__ENV__%",
    "debug": true,
    "app.package": "Tables4dms",
    "database": {
        "db.options": {
            "driver": "pdo_mysql",
            "host": "192.168.99.100",
            "port": "3306",
            "dbname": "rpgtables",
            "user": "rpgtables",
            "password": "upzone",
            "charset": "utf8"
        }
    },
    "migrations": {
        "migrations.name": "Tables4DMs migrations",
        "migrations.table_name": "tables4dms_migrations",
        "migrations.directory": "app/migrations",
        "migrations.namespace": "Tables4dms\\Migrations"
    },
    "orm": {
        "orm.proxies_dir": "../cache/proxies",
        "orm.auto_generate_proxies": true,
        "orm.em.options": {
            "mappings": {
                "0": {
                    "type": "annotation",
                    "namespace": "Tables4dms\\Entity",
                    "path": "src/Tables4dms/Entity",
                    "use_simple_annotation_reader": false
                }
            }
        }
    },
    "swagger": {
        "swagger.servicePath": "../src/Tables4dms"
    },
    "locale.lang": {
        "en": "../app/locale/en.yml",
        "pt_br": "../app/locale/pt_br.yml"
    },
    "locale.fallback": "en"
}

